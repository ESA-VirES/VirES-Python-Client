<?xml version="1.0"?>
<wps:Execute xmlns:wps="http://www.opengis.net/wps/1.0.0" xmlns:ows="http://www.opengis.net/ows/1.1" xmlns:xlink="http://www.w3.org/1999/xlink" version="1.0.0" service="WPS">
  <ows:Identifier>vires:eval_model_at_time_and_location</ows:Identifier>
  <wps:DataInputs>
    <wps:Input>
      <ows:Identifier>model_ids</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{model_expression | cdata}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
    <wps:Input>
      <ows:Identifier>input</ows:Identifier>
      <wps:Reference mimeType="{{input_mime_type}}" xlink:href="cid:{{input_content_id}}"/>
    </wps:Input>
{% if input_time_format -%}
    <wps:Input>
      <ows:Identifier>input_time_format</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{input_time_format}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
{% endif -%}
{% if output_time_format -%}
    <wps:Input>
      <ows:Identifier>output_time_format</ows:Identifier>
      <wps:Data>
        <wps:LiteralData>{{output_time_format}}</wps:LiteralData>
      </wps:Data>
    </wps:Input>
{% endif -%}
  </wps:DataInputs>
  <wps:ResponseForm>
    <wps:RawDataOutput mimeType="{{output_mime_type}}">
      <ows:Identifier>output</ows:Identifier>
    </wps:RawDataOutput>
  </wps:ResponseForm>
</wps:Execute>
